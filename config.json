{
	"PORT": 5000,

	"general": {
		"title": "YouTransfer",
		"subtitle": "The self-hosted file sharing solution",
		"baseUrl": "https://s.dam.vc",
		"cleanupSchedule": "*/1 * * * *",
		"schedulerEnabled": false,
		"enableDownload": true
	},

	"dropzone": {
		"maxFilesize": "2000",
		"parallelUploads": 20,
		"forceFallback": false
	},

	"email": {
		"subject": "YouTransfer",
		"transporter": "direct",
		"sendmailPath": "/usr/sbin/sendmail",
		"region": "eu-west-1",
		"rateLimit": 1
	},

	"storage": {
		"location": "local",
		"localstoragepath": "/content/drive/My Drive/share/",
		"retention": 1,
		"retentionUnit": "weeks",
		"S3Region": "us-east-1",
		"S3SSLEnabled": "true"
	},

	"state": {}

}
